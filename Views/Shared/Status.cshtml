@model StatusCodeModel
@{
  Layout = null;
  ViewBag.Title = Model.Title;
}
<!doctype html>
<html lang="en">
<head>
  <partial name="_Head" />
</head>
<body>

  <div class="min-vh-100 d-flex flex-column justify-content-center align-items-center p-5">
    <h1 class="http">@Model.Code</h1>
    <h2 class="mb-3">@Model.Title</h2>
    <p class="mb-5">@Model.Detail</p>
    <a class="btn btn-primary" href="~/">Go Home</a>
  </div>

</body>
</html>


